{
	"name": "octobug",
	"version": "1.0",
	"description": "what if eggbug was a GitHub Pages-based web-postable Eleventy-powered monstrosity",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"start": "npx @11ty/eleventy --serve --quiet --port=5080",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"author": {
		"name": "topghost",
		"email": "topghost@fastmail.com",
		"url": "https://topposts.net"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0-beta.1",
		"@11ty/eleventy-img": "^3.1.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^2.0.0",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"eleventy-plugin-embed-everything": "^1.19.0",
		"luxon": "^3.5.0",
		"markdown-it-anchor": "^9.2.0"
	}
}
